    "@vitest/ui": "^1.1.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "exports": {
    ".": "./dist/cpc/cpc.js",
    "./node": "./dist/node/node.js",
    "./web": "./dist/web/web.js"
  },
  "imports": {
    "#lib/*": "./dist/cpc/lib/*"
