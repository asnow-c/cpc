{
  "name": "cpcall",
  "version": "0.4.3",
  "description": "",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --ui",
    "build": "pnpm rollup -c && pnpm gen-dts && pnpm api:check",
    "api:check": "pnpm run ci:check-api -l",
    "ci:build": "pnpm rollup -c && pnpm gen-dts true",
    "ci:test": "vitest run",
    "gen-dts": "node scripts/build_type.js",
    "ci:check-api": "api-extractor run -c api-extractor.jsonc -v"
  },
  "dependencies": {
    "jbod": "^0.4.5"
  },
  "devDependencies": {
    "@eavid/lib-dev": "^0.0.3",
    "@microsoft/api-extractor": "^7.43.1",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "evlib": "^2.1.0",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "types": "./dist/cpc.d.ts",
  "exports": {
    ".": "./dist/cpc.js",
    "./node": "./dist/node.js",
    "./web": "./dist/web.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/asnowc/cpcall"
  },
  "license": "MIT",
  "author": {
    "name": "Eavid",
    "url": "https://github.com/eavidy"
  },
  "engines": {
    "node": ">=18"
  }
}
