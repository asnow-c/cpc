    "@vitest/ui": "^1.2.2",
    "evlib": "^1.6.0",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "exports": {
