{
  "name": "cpcall",
  "version": "0.5.2",
  "description": "",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --ui",
    "check-type": "tsc --noEmit -p src/tsconfig.json",
    "build": "pnpm rollup -c --environment DEV && pnpm gen-dts && pnpm api-extract",
    "api-extract": "pnpm run ci:check-api -l",
    "gen-dts": "node build/build_type.js",
    "ci:build": "pnpm rollup -c && pnpm gen-dts true",
    "ci:test": "vitest run",
    "ci:check-api": "api-extractor run -c api-extractor.jsonc -v"
  },
  "dependencies": {
    "jbod": "^0.5.0"
  },
  "devDependencies": {
    "@eavid/lib-dev": "^0.0.3",
    "@microsoft/api-extractor": "^7.47.0",
    "@types/node": "^20.14.10",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "evlib": "^2.6.1",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  },
  "types": "./dist/cpc.d.ts",
  "exports": {
    ".": "./dist/mod.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "provenance": true
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/asnowc/cpcall"
  },
  "license": "MIT",
  "author": {
    "name": "Eavid",
    "url": "https://github.com/eavidy"
  },
  "engines": {
    "node": ">=18"
  }
}
