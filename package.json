{
    "name": "cpcall",
    "version": "0.0.1-3",
    "description": "",
    "type": "module",
    "scripts": {
        "build": "tsc -p tsconfig.build.json && tsc -p web/tsconfig.json",
        "build:line": "pnpm build && pnpm api:extractor-all && pnpm publish:test",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage --ui",
        "ci:test": "vitest run --coverage",
        "ci:build": "pnpm build",
        "publish:test": "pnpm publish --no-git-checks --dry-run",
        "api:extractor": "api-extractor run -vc",
        "api:extractor-all": "pnpm api:extractor api/api-e.jbson.json && pnpm api:extractor api/api-e.cpc.json && pnpm api:extractor api/api-e.node.json &&  pnpm api:extractor api/api-e.web.json "
    },
    "devDependencies": {
        "@microsoft/api-extractor": "^7.36.4",
        "@types/node": "^20.4.4",
        "@vitest/coverage-v8": "^0.33.0",
        "@vitest/ui": "^0.33.0",
        "vitest": "^0.33.0"
    },
    "exports": {
        ".": "./out/cpc.js",
        "./lib/*": "./out/lib/*",
        "./node": "./out/node.js",
        "./web": "./ws_cpc.js"
    },
    "imports": {
        "#rt/*": "./out/*",
        "#lib/*": "./out/lib/*"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "files": [
        "**/out/**/*.js",
        "**/out/**/*d.ts"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/asnow-c/cpcall"
    }
}
