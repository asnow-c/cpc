{
    "name": "cpcall",
    "version": "0.0.2",
    "description": "",
    "author": {
        "name": "Eavid",
        "url": "https://github.com/ieavid"
    },
    "type": "module",
    "scripts": {
        "ci:build": "tsc -p tsconfig.build.json",
        "publish:test": "pnpm publish --no-git-checks --dry-run",
        "api:extractor": "api-extractor run -v",
        "api:extractor-local": "pnpm run api:extractor -lc api-extractor.json"
    },
    "devDependencies": {
        "@microsoft/api-extractor": "^7.36.4",
        "@types/node": "^20.4.4"
    },
    "exports": {
        ".": "./out/core/cpc.js",
        "./node": "./out/node/index.js",
        "./web": "./out/web/index.js"
    },
    "imports": {
        "#lib/*": "./out/core/lib/*"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "files": [
        "**/out/**/*.js",
        "**/out/**/*d.ts"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ieavid/cpcall"
    },
    "dependencies": {
        "@eavid/js-bson": "workspace:^"
    }
}
